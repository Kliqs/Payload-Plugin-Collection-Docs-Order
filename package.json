{
  "name": "vs-payload-plugin-collection-docs-order",
  "version": "1.1.4",
  "description": "A Payload plugin that allows dragging to order your collections in the Payload admin panel.",
  "author": "dev@payloadcms.com",
  "license": "MIT",
  "maintainers": [
    {
      "name": "dev",
      "email": "dev@extravigin.ch"
    }
  ],
  "type": "module",
  "scripts": {
    "build": "pnpm build:types && pnpm build:swc",
    "build:swc": "pnpm exec swc ./src -d ./dist --config-file .swcrc --strip-leading-paths",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "clean": "rimraf {dist,*.tsbuildinfo}",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "echo \"No tests available.\""
  },
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@payloadcms/ui": "^3.4.0",
    "@radix-ui/react-dialog": "^1.1.4",
    "add": "^2.0.6",
    "clsx": "^2.1.1",
    "escape-html": "^1.0.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-toastify": "^11.0.2"
  },
  "devDependencies": {
    "@payloadcms/eslint-config": "3.0.0",
    "@swc/cli": "^0.5.2",
    "@swc/core": "^1.10.1",
    "@types/escape-html": "^1.0.4",
    "@types/react": "18.0.0",
    "@types/react-dom": "18.0.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "payload": "3.4.0",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "payload": "3.4.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./types": {
      "import": "./dist/exports/types.js",
      "types": "./dist/exports/types.d.ts",
      "default": "./dist/exports/types.js"
    },
    "./client": {
      "import": "./dist/exports/client.js",
      "types": "./dist/exports/client.d.ts",
      "default": "./dist/exports/client.js"
    },
    "./components/CollectionDocsOrder": {
      "import": "./dist/exports/components/CollectionDocsOrder.js",
      "types": "./dist/exports/components/CollectionDocsOrder.d.ts",
      "default": "./dist/exports/components/CollectionDocsOrder.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kliqs/Payload-Plugin-Collection-Docs-Order.git"
  },
  "keywords": [
    "payload",
    "cms",
    "plugin",
    "typescript",
    "react",
    "forms",
    "fields",
    "drag drop",
    "order"
  ],
  "bugs": {
    "url": "https://github.com/Kliqs/payload-plugin-collection-docs-order/issues"
  },
  "homepage": "https://github.com/Kliqs/payload-plugin-collection-docs-order#readme",
  "packageManager": "pnpm@9.15.0+sha512.76e2379760a4328ec4415815bcd6628dee727af3779aaa4c914e3944156c4299921a89f976381ee107d41f12cfa4b66681ca9c718f0668fa0831ed4c6d8ba56c"
}
